_nextBlock	memMgr.h	/^   MemBlock<T>*      _nextBlock;$/;"	m	class:MemBlock
_nextList	memMgr.h	/^   MemRecycleList<T>*  _nextList;  \/\/ next MemRecycleList$/;"	m	class:MemRecycleList
